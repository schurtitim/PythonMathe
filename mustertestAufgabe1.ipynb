   "source": [
    "def q_rekursiv(n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    else:\n",
    "        return q_rekursiv(n-1) + 2**(n-1)\n",
    "\n",
    "def q_explizit(n):\n",
    "    return 2**n - 1\n",
    "\n",
    "# Beispiel: Werte f√ºr n = 0 bis 5\n",
    "for n in range(6):\n",
    "    print(f\"n={n}: rekursiv={q_rekursiv(n)}, explizit={q_explizit(n)}\")"
   ]
  }
 ],
